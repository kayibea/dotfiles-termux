set_prompt_emoji() {
    local positive_emojis=("ğŸŒŸ" "ğŸ‰" "ğŸš€" "ğŸˆ" "ğŸ”¥" "ğŸ’" "ğŸ¥³" "ğŸ˜Š" "âœ¨")
    local negative_emojis=("ğŸ˜" "ğŸ˜±" "ğŸ’”" "ğŸ˜¢" "ğŸ‘" "ğŸ˜”" "ğŸ˜’" "ğŸ˜ " "ğŸ¤¦")

    local exit_code=$?
    if [ $exit_code -eq 0 ]; then
        echo "${positive_emojis[RANDOM % ${#positive_emojis[@]}]}"
    else
        echo "${negative_emojis[RANDOM % ${#negative_emojis[@]}]}"
    fi
}
